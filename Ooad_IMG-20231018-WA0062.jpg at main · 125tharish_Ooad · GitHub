.ToggleSwitch-track[aria-pressed="true"]:not([disabled]):active { background-color: var(--control-checked-bgColor-active, var(--color-switch-track-checked-active-bg)); }

.ToggleSwitch-track[aria-pressed="true"] .ToggleSwitch-knob { background-color: var(--controlKnob-bgColor-checked, var(--color-switch-knob-checked-bg)); border-color: var(--controlKnob-borderColor-checked, var(--color-switch-knob-checked-border)); transform: translateX(100%); }

